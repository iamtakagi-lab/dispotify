{
  "name": "iamtakagi.net",
  "author": "takagi",
  "license": "MIT",
  "scripts": {
    "build:client": "next build src/client",
    "build:server": "parcel build --target node src/index.ts",
    "dev": "yarn build:client && NODE_ENV=production ts-node-dev --respawn src/index.ts",
    "build": "run-p -l build:*",
    "start": "NODE_ENV=production node dist/index.js",
    "lint:prettier": "prettier . --check",
    "lint:eslint": "eslint . --format friendly",
    "lint": "run-p lint:*",
    "fix:prettier": "yarn lint:prettier --write",
    "fix:eslint": "yarn lint:eslint --fix",
    "fix": "run-s fix:eslint fix:prettier",
    "prisma:generate": "npx prisma generate",
    "prisma:migrate": "npx prisma migrate dev --name init",
    "test": "jest",
    "test:watch": "jest --coverage --silent=false --verbose false --watch",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@koa/cors": "^3.1.0",
    "@prisma/client": "2.26.0",
    "@react-icons/all-files": "^4.1.0",
    "@types/koa-router": "^7.4.2",
    "@types/lru-cache": "^5.1.0",
    "axios": "^0.21.1",
    "date-fns": "^2.22.1",
    "discord.js": "^12.5.3",
    "http-status": "^1.5.0",
    "koa": "^2.13.1",
    "koa-bodyparser": "^4.3.0",
    "koa-helmet": "^6.1.0",
    "koa-logger": "^3.2.1",
    "koa-requestid": "^2.1.0",
    "koa-router": "^10.0.0",
    "koa-static": "^5.0.0",
    "next": "^10.0.7",
    "npm-run-all": "^4.1.5",
    "react": "^17.0.1",
    "react-cookie": "^4.0.3",
    "react-dom": "^17.0.1",
    "react-icons": "^4.2.0",
    "react-switch": "^6.0.0",
    "react-toast-notifications": "^2.4.4",
    "spotify-web-api-node": "^5.0.2",
    "use-dark-mode": "^2.3.1"
  },
  "devDependencies": {
    "@tailwindcss/custom-forms": "^0.2.1",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.10",
    "@types/expect": "^24.3.0",
    "@types/express": "^4.17.12",
    "@types/express-session": "1.17.0",
    "@types/jest": "^26.0.23",
    "@types/koa": "^2.13.3",
    "@types/koa-bodyparser": "^4.3.1",
    "@types/koa-helmet": "^6.0.3",
    "@types/koa-logger": "^3.1.1",
    "@types/koa-requestid": "^2.0.2",
    "@types/koa__cors": "^3.0.2",
    "@types/node": "14.14.22",
    "@types/react": "17.0.0",
    "@types/spotify-web-api-node": "^5.0.2",
    "@typescript-eslint/eslint-plugin": "^4.26.0",
    "@typescript-eslint/parser": "^4.26.0",
    "autoprefixer": "10.2.3",
    "eslint": "^7.27.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-react": "1.1.7",
    "eslint-formatter-friendly": "7.0.0",
    "eslint-plugin-react": "7.22.0",
    "eslint-plugin-react-hooks": "4.2.0",
    "jest": "^27.0.4",
    "parcel-bundler": "^1.12.5",
    "postcss": "8.2.4",
    "prettier": "^2.3.0",
    "prisma": "2.26.0",
